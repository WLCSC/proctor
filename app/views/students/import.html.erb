<div class="page-header">
	<h1>Import Students</h1>
</div>

<%= form_tag import_students_path do %>
	<%= label_tag :names, "Student Names" %>
	<span class="help-block">1 Name per Line</span>
	<%= text_area_tag :names %>
	<br/>
	<%= submit_tag "Import", :class => 'btn btn-large btn-success' %>
<% end %>
